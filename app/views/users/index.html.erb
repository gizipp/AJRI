<h1>Manage User</h1>

<table>
  <thead>
    <tr>
      <th width="80">Username</th>
      <th width="150">Role</th>
    </tr>
  </thead>
    <tbody>
        <% @users.each do |user| %>
          <tr>
            <td><%= link_to user.username, users_path %></td>
            <td><%= user.role %></td>
          </tr>
        <% end %>
      </tbody>
</table>

<%= paginate @users %>